{
  "name": "misterioso013/cpf",
  "description": "Simples Gerador e Validador de CPF PHP",
  "minimum-stability": "stable",
  "homepage": "https://github.com/misterioso013/cpf",
  "license": "MIT",
  "keywords": [
    "CFP Generator",
    "CPF Validate",
    "CPF"
  ],
  "authors": [
    {
      "name": "Rosiel Victor",
      "email": "rosielvictor.dev@gmail.com",
      "homepage": "https://github.com/misterioso013",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Misterioso013\\Tools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Misterioso013\\Tools\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "test": "phpunit",
    "cs-fix": "php-cs-fixer fix --allow-risky=yes",
    "analyse": "phpstan analyse src tests",
    "check": [
      "@cs-fix",
      "@analyse",
      "@test"
    ]
  }
}